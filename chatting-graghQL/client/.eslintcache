\n    {({ loading, data, subscribeToMore }) => {\n      if (loading) {\n        return null;\n      }\n      if (!unsubscribe) {\n        unsubscribe = subscribeToMore({\n          document: newChat,\n          updateQuery: (prev, { subscriptionData }) => {\n            if (!subscriptionData.data) return prev;\n            const { newChat } = subscriptionData.data;\n            return {\n              ...prev,\n              chatting: [...prev.chatting, newChat],\n            };\n          },\n        });\n      }\n      return (\n        <div>\n          {data.chatting.map((x) => (\n            <h3 key={x.id}>\n              {x.writer}: {x.description}\n            </h3>\n          ))}\n        </div>\n      );\n    }}\n  </Query>\n);\n","/Users/dong/Documents/devkdh_study/react-board/chatting-graghQL/client/src/App.js",[],"/Users/dong/Documents/devkdh_study/react-board/chatting-graghQL/client/src/index.js",[],"/Users/dong/Documents/devkdh_study/react-board/chatting-graghQL/client/src/apolloClient.js",[],{"ruleId":"24","severity":1,"message":"25","line":27,"column":1,"nodeType":"26","endLine":57,"endColumn":3},"import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration"]